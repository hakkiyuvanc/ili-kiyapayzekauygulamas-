    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "export": "next build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run export && electron-builder",
    "build:desktop": "npm run export && electron-builder",
    "build:mac": "npm run export && electron-builder --mac",
    "build:win": "npm run export && electron-builder --win",
    "build:linux": "npm run export && electron-builder --linux",
    "build:all": "npm run export && electron-builder --mac --win --linux",
    "dist": "npm run build:desktop",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps"
  },
